{
  "name": "angular-client",
  "version": "0.0.1",
  "scripts": {
    "rimraf": "rimraf",
    "clean": "rimraf ./node_modules",
    "npm": "npm",
    "restore:node-sass": "npm rebuild node-sass",
    "restore": "npm install && npm run restore:node-sass",
    "restore:no-cache": "npm run clean && npm run restore",
    "ng": "ng",
    "lint": "ng lint",
    "build": "ng build",
    "build:dev": "ng build --watch",
    "build:ssr": "ng run angular-client:server:dev",
    "start": "ng serve",
    "start:docker": "ng serve --host 0.0.0.0 --disableHostCheck"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.3",
    "@angular/common": "^8.2.3",
    "@angular/compiler": "^8.2.3",
    "@angular/core": "^11.0.5",
    "@angular/forms": "^8.2.3",
    "@angular/platform-browser": "^8.2.3",
    "@angular/platform-browser-dynamic": "^8.2.3",
    "@angular/platform-server": "^8.2.3",
    "@angular/router": "^8.2.3",
    "core-js": "^2.6.9",
    "rxjs": "~6.5.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.802.2",
    "@angular/cli": "^8.2.2",
    "@angular/compiler-cli": "^8.2.3",
    "@angular/language-service": "^8.2.3",
    "@types/node": "^8.10.49",
    "codelyzer": "^5.0.1",
    "node-sass": "^4.12.0",
    "rimraf": "^2.6.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.18.0",
    "typescript": "~3.5.3"
  }
}
